{
  "name": "keri-cli",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix && prettier --write .",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "node --test --test-reporter=spec --no-warnings --experimental-strip-types src/**/*.test.ts",
    "test:only": "node --test --test-only --test-reporter=spec --no-warnings --experimental-strip-types src/**/*.test.ts"
  },
  "bin": {
    "keri": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "keri": "workspace:*",
    "@kerijs/db-sqlite": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.0"
  }
}
